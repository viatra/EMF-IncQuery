            id="org.eclipse.viatra2.emf.incquery.tooling.gui.loadmodel"
            name="Load model">
      </command>
      <command
            categoryId="org.eclipse.viatra2.emf.incquery.tooling.category"
            id="org.eclipse.viatra2.emf.incquery.tooling.gui.showlocation"
            name="Show location">
      </command>
      <category
            description="Commands related to EMF-IncQuery"
            id="org.eclipse.viatra2.emf.incquery.tooling.category"
            name="EMF-IncQuery">
      </category>
      <command
            categoryId="org.eclipse.viatra2.emf.incquery.tooling.category"
            id="org.eclipse.viatra2.emf.incquery.tooling.gui.unmodel"
            name="Unload model">
