<extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.viatra2.emf.incquery.tooling.gui"
            name="EMF-IncQuery">
      </category>
      <view
            category="org.eclipse.viatra2.emf.incquery.tooling.gui"
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.QueryExplorer"
            icon="icons/logo2.png"
            id="org.eclipse.viatra2.emf.incquery.queryexplorer.QueryExplorer"
            name="Query Explorer">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="org.eclipse.viatra2.emf.incquery.queryexplorer.QueryExplorer"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.pde.ui.PDEPerspective">
         <view
               id="org.eclipse.viatra2.emf.incquery.queryexplorer.QueryExplorer"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.eclipse.viatra2.patternlanguage.emf.file">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <adapt
                     type="org.eclipse.core.resources.IFile">
                  <test
                        property="org.eclipse.core.resources.name"
                        value="*.eiq">
                  </test>
               </adapt>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra2.emf.incquery.tooling.gui.root">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra2.emf.incquery.queryexplorer.content.matcher.ObservablePatternMatcherRoot">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra2.emf.incquery.tooling.gui.match">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra2.emf.incquery.queryexplorer.content.matcher.ObservablePatternMatch">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra2.emf.incquery.tooling.gui.matcher">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra2.emf.incquery.queryexplorer.content.matcher.ObservablePatternMatcher">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra2.patternlanguage.emf.patterncomponent">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.viatra2.emf.incquery.queryexplorer.content.patternsviewer.PatternComponent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra2.emf.incquery.tooling.gui.activedomaineditor">
         <with
               variable="activeEditor">
            <instanceof
                  value="org.eclipse.emf.edit.domain.IEditingDomainProvider">
            </instanceof>
         </with>
      </definition>
      <definition
            id="org.eclipse.viatra2.emf.incquery.tooling.gui.activepatterneditor">
         <with
               variable="activeEditorId">
            <equals
                  value="org.eclipse.viatra2.patternlanguage.EMFPatternLanguage">
            </equals>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.RuntimeMatcherRegistrationHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.runtimematcherreg">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.LoadModelHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadmodel">
         <activeWhen>
            <reference
                  definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.activedomaineditor">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.LoadEiqPatternHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadmodel">
         <activeWhen>
            <reference
                  definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.activepatterneditor">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.ShowLocationHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.showlocation">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.UnloadModelHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.unmodel">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.PatternUnregistrationHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.unregisterpattern">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.PackagePresentationHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.pattern.flat">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.PackagePresentationHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.pattern.hierarchical">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.LoadModelHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadresourceset">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.LoadResourceHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadresource">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.LoadEiqModelHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadeiqmodel">
      </handler>
      <handler
            class="org.eclipse.viatra2.emf.incquery.gui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.viatra2.emf.incquery.queryexplorer.handlers.LoadEiqPatternHandler"
            commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadeiqpattern">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any">
         <menu
               id="org.eclipse.viatra2.emf.incquery.tooling.gui.navigator.popup"
               label="EMF-IncQuery">
            <command
                  commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.runtimematcherreg"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <reference
                        definitionId="org.eclipse.viatra2.patternlanguage.emf.file">
                  </reference>
               </visibleWhen>
            </command>
            <separator
                  name="additions"
                  visible="true">
            </separator>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.viatra2.emf.incquery.queryexplorer.QueryExplorer">
         <command
               commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadmodel"
               icon="icons/load.gif"
               id="org.eclipse.viatra2.emf.incquery.tooling.gui.loadmodelmenu"
               label="Load model"
               style="pulldown">
         </command>
         <separator
               name="org.eclipse.viatra2.emf.incquery.tooling.gui.separator1"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.viatra2.emf.incquery.queryexplorer.QueryExplorer.treeViewerMenu">
         <command
               commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.showlocation"
               label="Show location"
               style="push">
            <visibleWhen
                  checkEnabled="false">
                  <or>
                    <reference
                       definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.match">
                    </reference>
                    <reference
                       definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.matcher">
                    </reference>
                  </or>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.unmodel"
               icon="icons/unload.gif"
               label="Unload model"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.root">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:#TextEditorContext?after=group.edit">
         <menu
               id="org.eclipse.viatra2.emf.incquery.tooling.gui.menu.emfpatternlanguage"
               label="EMF-IncQuery">
            <command
                  commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.runtimematcherreg"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeEditorId">
                  <equals
                        value="org.eclipse.viatra2.patternlanguage.EMFPatternLanguage">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.viatra2.emf.incquery.queryexplorer.QueryExplorer.patternsViewerMenu">
         <command
               commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.unregisterpattern"
               icon="icons/unload.gif"
               label="Unregister pattern"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.viatra2.emf.incquery.queryexplorer.QueryExplorer">
         <menu
               label="Patterns viewer presentation">
            <command
                  commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.pattern.hierarchical"
                  icon="icons/hierarchicalLayout.gif"
                  label="Hierarchical"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.pattern.flat"
                  icon="icons/flatLayout.gif"
                  label="Flat"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.viatra2.emf.incquery.tooling.gui.loadmodelmenu">
         <command
               commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadresourceset"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.activedomaineditor">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadresource"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.activedomaineditor">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadeiqpattern"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.activepatterneditor">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.viatra2.emf.incquery.tooling.gui.loadeiqmodel"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.viatra2.emf.incquery.tooling.gui.activepatterneditor">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
