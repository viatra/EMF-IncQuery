<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.incquery.tooling.ui.category"
            name="EMF-IncQuery">
      </category>
      <wizard
            category="org.eclipse.incquery.tooling.ui.category"
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.wizards.NewProjectWizard"
            finalPerspective="org.eclipse.pde.ui.PDEPerspective"
            icon="icons/logo2.png"
            id="org.eclipse.incquery.tooling.ui.newproject"
            name="EMF-IncQuery Project"
            preferredPerspectives="org.eclipse.pde.ui.PDEPerspective,org.eclipse.jdt.ui.JavaPerspective"
            project="true">
      </wizard>
      <wizard
            category="org.eclipse.incquery.tooling.ui.category"
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.wizards.NewEiqFileWizard"
            finalPerspective="org.eclipse.pde.ui.PDEPerspective"
            icon="icons/logo2.png"
            id="org.eclipse.incquery.tooling.ui.neweiqfile"
            name="EMF-IncQuery Query Definition "
            preferredPerspectives="org.eclipse.pde.ui.PDEPerspective,org.eclipse.jdt.ui.JavaPerspective">
      </wizard>
      <wizard
            category="org.eclipse.incquery.tooling.ui.category"
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.wizards.NewGenmodelWizard"
            icon="icons/logo2.png"
            id="org.eclipse.incquery.tooling.ui.neweiqgenmodel"
            name="EMF-IncQuery Generator Model"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            id="org.eclipse.incquery"
            name="EMF-IncQuery">
      </category>
      <cheatsheet
            category="org.eclipse.incquery"
            composite="true"
            contentFile="cheatsheets/gettingstarted.xml"
            id="org.eclipse.incquery.tooling.ui.cheatsheet"
            name="Getting Started with EMF-IncQuery">
      </cheatsheet>
   </extension>
<extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.incquery.tooling.ui"
            name="EMF-IncQuery">
      </category>
      <view
            category="org.eclipse.incquery.tooling.ui"
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.QueryExplorer"
            icon="icons/logo2.png"
            id="org.eclipse.incquery.tooling.ui.queryexplorer.QueryExplorer"
            name="Query Explorer">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="org.eclipse.incquery.tooling.ui.queryexplorer.QueryExplorer"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
         <newWizardShortcut
               id="org.eclipse.incquery.tooling.ui.neweiqgenmodel">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.incquery.tooling.ui.neweiqfile">
         </newWizardShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.pde.ui.PDEPerspective">
         <view
               id="org.eclipse.incquery.tooling.ui.queryexplorer.QueryExplorer"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
         <newWizardShortcut
               id="org.eclipse.incquery.tooling.ui.neweiqgenmodel">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.incquery.tooling.ui.neweiqfile">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.eclipse.incquery.tooling.ui.eiq.file">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <adapt
                     type="org.eclipse.core.resources.IFile">
                  <test
                        property="org.eclipse.core.resources.name"
                        value="*.eiq">
                  </test>
               </adapt>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.incquery.tooling.ui.root">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.incquery.tooling.ui.queryexplorer.content.matcher.ObservablePatternMatcherRoot">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.incquery.tooling.ui.match">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.incquery.tooling.ui.queryexplorer.content.matcher.ObservablePatternMatch">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.incquery.tooling.ui.matcher">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.incquery.tooling.ui.queryexplorer.content.matcher.ObservablePatternMatcher">
               </instanceof>
            </iterate>
         </with>
      </definition>
      <definition
            id="org.eclipse.incquery.tooling.ui.patterncomponent">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false">
               <instanceof
                     value="org.eclipse.incquery.tooling.ui.queryexplorer.content.patternsviewer.PatternComponent">
               </instanceof>
            </iterate>
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="org.eclipse.incquery.tooling.ui.activedomaineditor">
         <with
               variable="activeEditor">
            <instanceof
                  value="org.eclipse.emf.edit.domain.IEditingDomainProvider">
            </instanceof>
         </with>
      </definition>
      <definition
            id="org.eclipse.incquery.tooling.ui.activepatterneditor">
         <with
               variable="activeEditorId">
            <equals
                  value="org.eclipse.incquery.patternlanguage.emf.EMFPatternLanguage">
            </equals>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.RuntimeMatcherRegistrationHandler"
            commandId="org.eclipse.incquery.tooling.ui.runtimematcherreg">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.LoadModelHandler"
            commandId="org.eclipse.incquery.tooling.ui.loadmodel">
         <activeWhen>
            <reference
                  definitionId="org.eclipse.incquery.tooling.ui.activedomaineditor">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.LoadEiqPatternHandler"
            commandId="org.eclipse.incquery.tooling.ui.loadmodel">
         <activeWhen>
            <reference
                  definitionId="org.eclipse.incquery.tooling.ui.activepatterneditor">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.ShowLocationHandler"
            commandId="org.eclipse.incquery.tooling.ui.showlocation">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.queryexplorer.handlers.UnloadModelHandler"
            commandId="org.eclipse.incquery.tooling.ui.unmodel">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.queryexplorer.handlers.PatternUnregistrationHandler"
            commandId="org.eclipse.incquery.tooling.ui.unregisterpattern">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.queryexplorer.handlers.PackagePresentationHandler"
            commandId="org.eclipse.incquery.tooling.ui.pattern.flat">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.queryexplorer.handlers.PackagePresentationHandler"
            commandId="org.eclipse.incquery.tooling.ui.pattern.hierarchical">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.LoadModelHandler"
            commandId="org.eclipse.incquery.tooling.ui.loadresourceset">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.LoadResourceHandler"
            commandId="org.eclipse.incquery.tooling.ui.loadresource">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.LoadEiqModelHandler"
            commandId="org.eclipse.incquery.tooling.ui.loadeiqmodel">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.LoadEiqPatternHandler"
            commandId="org.eclipse.incquery.tooling.ui.loadeiqpattern">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.IncQueryLanguageExecutableExtensionFactory:org.eclipse.incquery.tooling.ui.queryexplorer.handlers.PatternRegitryShowLocationHandler"
            commandId="org.eclipse.incquery.tooling.ui.showlocation_patternreg">
      </handler>
      <handler
            class="org.eclipse.incquery.tooling.ui.queryexplorer.handlers.ResetUIHandler"
            commandId="org.eclipse.incquery.tooling.ui.resetui">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any">
         <menu
               id="org.eclipse.incquery.tooling.ui.navigator.popup"
               label="EMF-IncQuery">
            <command
                  commandId="org.eclipse.incquery.tooling.ui.runtimematcherreg"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <reference
                        definitionId="org.eclipse.incquery.tooling.ui.eiq.file">
                  </reference>
               </visibleWhen>
            </command>
            <separator
                  name="additions"
                  visible="true">
            </separator>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.incquery.tooling.ui.queryexplorer.QueryExplorer">
         <command
               commandId="org.eclipse.incquery.tooling.ui.loadmodel"
               icon="icons/load.gif"
               id="org.eclipse.incquery.tooling.ui.loadmodelmenu"
               label="Load model"
               style="pulldown">
         </command>
         <separator
               name="org.eclipse.incquery.tooling.ui.separator1"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.incquery.tooling.ui.queryexplorer.QueryExplorer.treeViewerMenu">
         <command
               commandId="org.eclipse.incquery.tooling.ui.showlocation"
               icon="icons/forward_nav.gif"
               label="Show location"
               style="push">
            <visibleWhen
                  checkEnabled="false">
                  <or>
                    <reference
                       definitionId="org.eclipse.incquery.tooling.ui.match">
                    </reference>
                    <reference
                       definitionId="org.eclipse.incquery.tooling.ui.matcher">
                    </reference>
                  </or>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.incquery.tooling.ui.unmodel"
               icon="icons/unload.gif"
               label="Unload model"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.incquery.tooling.ui.root">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:#TextEditorContext?after=group.edit">
         <menu
               id="org.eclipse.incquery.tooling.ui.menu.emfpatternlanguage"
               label="EMF-IncQuery">
            <command
                  commandId="org.eclipse.incquery.tooling.ui.runtimematcherreg"
                  style="push">
            </command>
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeEditorId">
                  <equals
                        value="org.eclipse.incquery.patternlanguage.emf.EMFPatternLanguage">
                  </equals>
               </with>
            </visibleWhen>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.incquery.tooling.ui.queryexplorer.QueryExplorer.patternsViewerMenu">
         <command
               commandId="org.eclipse.incquery.tooling.ui.unregisterpattern"
               icon="icons/unload.gif"
               label="Unregister pattern"
               style="push">
         </command>
         <command
               commandId="org.eclipse.incquery.tooling.ui.showlocation_patternreg"
               icon="icons/forward_nav.gif"
               label="Show location"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.incquery.tooling.ui.queryexplorer.QueryExplorer">
         <menu
               label="Patterns viewer presentation">
            <command
                  commandId="org.eclipse.incquery.tooling.ui.pattern.hierarchical"
                  icon="icons/hierarchicalLayout.gif"
                  label="Hierarchical"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.incquery.tooling.ui.pattern.flat"
                  icon="icons/flatLayout.gif"
                  label="Flat"
                  style="push">
            </command>
         </menu>
         <command
               commandId="org.eclipse.incquery.tooling.ui.resetui"
               icon="icons/reset.gif"
               label="Reset UI"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.incquery.tooling.ui.loadmodelmenu">
         <command
               commandId="org.eclipse.incquery.tooling.ui.loadresourceset"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.incquery.tooling.ui.activedomaineditor">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.incquery.tooling.ui.loadresource"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.incquery.tooling.ui.activedomaineditor">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.incquery.tooling.ui.loadeiqpattern"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.incquery.tooling.ui.activepatterneditor">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.incquery.tooling.ui.loadeiqmodel"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.eclipse.incquery.tooling.ui.activepatterneditor">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
